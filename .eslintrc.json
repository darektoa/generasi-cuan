{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-multiple-empty-lines": 0,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 0,
        "no-multi-spaces": 0,
        "require-jsdoc": 0,
        "padded-blocks": 0,
        "no-plusplus": 0,
        "no-continue": 0,
        "no-console": 0,
        "eol-last": 0,
        "max-len": 0,
        "new-cap": 0
    }
}
